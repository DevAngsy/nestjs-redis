{
  "compilerOptions": {
    "module": "Node16",
    "moduleResolution": "Node16",
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "lib": ["ES2023"],
    "esModuleInterop": true,
    "importHelpers": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
