{
    "name": "@liaoliao/nestjs-redis",
    "version": "0.1.0",
    "description": "Redis(ioredis) module for NestJS",
    "author": "LiaoLiao",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "license": "MIT",
    "keywords": [
        "redis",
        "ioredis",
        "cluster",
        "nestjs",
        "nestjs-redis"
    ],
    "homepage": "https://github.com/DevAngsy/nestjs-redis#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DevAngsy/nestjs-redis.git"
    },
    "bugs": {
        "url": "https://github.com/DevAngsy/nestjs-redis/issues"
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "concurrently --prefix index \"npm run prebuild\" \"tsc -p tsconfig.build.json\"",
        "lint": "eslint \"{src,tests}/**/*.ts\" --fix",
        "test": "jest",
        "test:cov": "jest --coverage"
    },
    "devDependencies": {
        "@nestjs/common": "^7.6.15",
        "@nestjs/core": "^7.6.15",
        "@nestjs/testing": "^7.6.15",
        "@types/jest": "^26.0.23",
        "@typescript-eslint/eslint-plugin": "^4.22.1",
        "@typescript-eslint/parser": "^4.22.1",
        "concurrently": "^6.1.0",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-tsdoc": "^0.2.14",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.5.6",
        "tslib": "^2.2.0",
        "typescript": "^4.2.4"
    },
    "peerDependencies": {
        "@types/ioredis": "^4.26.1",
        "ioredis": "^4.27.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "./",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "./coverage",
        "testEnvironment": "node",
        "verbose": true,
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/$1"
        }
    }
}
